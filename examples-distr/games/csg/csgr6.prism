csg

player p1
	[a], [b], [e], [f]
endplayer

player p2
	[c], [d]
endplayer

module m1
	s : [0..4];
	[a] (s=0) -> (s'=1);
	[b] (s=0) -> 1/2 : (s'=1) + 1/2: (s'=2);
	[e] (s<3 & t!=0) -> (s'=3);
	[f] (s=3) -> (s'=4);  
endmodule

module m2
	t : [0..2];
	[c] (s=0) -> (t'=2);
	[d] (s=0) -> (t'=1);
endmodule

module m3
	u : [0..1];
	[c] (s=0) -> 1/2 : (u'=1) + 1/2 : true;
	[d] (s=0) -> (u'=1);
endmodule

rewards "r1"
	[f] (s=3) & (t=1) : 1;
	[f] (s=3) & (t=2) : 2;
endrewards