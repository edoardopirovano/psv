agent
	atA : bool init true;
	[move] true : 0.5;
	[cleanA] atA : 0.5;
	[cleanB] !atA : 0.5;
	update
	    (move, atA, {}) -> (atA'=false);
	    (move, !atA, {}) -> (atA'=true);
	endupdate
endagent

environment
	a : bool init false;
	b : bool init false;
	update
	    (true, {cleanA}) -> (a'=true);
	    (true, {cleanB}) -> (b'=true);
	endupdate
endenvironment

label "clean" = a_E & b_E;